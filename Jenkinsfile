pipeline {
    agent any 
    stages {
        stage('First Stage Name') {
            steps {
                echo "One"
            }
        }
        stage('Second Stage Name') {
            steps {
                 echo "One"
            }
        }
        stage('Third Stage Name') {
            steps {
                 echo "Three"
            }
        }
    }
}









